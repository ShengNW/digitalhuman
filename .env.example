# 对外可达域名或IP（orchestrator 会用它生成 connect_url/base_url）
PUBLIC_HOST=example.com

# MinIO（按你们实际填，若无可先留空）
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=changeme
MINIO_SECRET_KEY=changeme
MINIO_BUCKET=yeying-interviewer
MINIO_SECURE=false

# 端口（习惯项，不必须改）
ORCH_PORT=9009
LLM_PORT=8011
VTUBER_PORT=12393
LOG_LEVEL=INFO
